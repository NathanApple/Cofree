<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.aol.PreviewController">
    <children>
        <Label prefHeight="98.0" prefWidth="158.0" text="Coupon : ">
         <font>
            <Font name="Javanese Text" size="35.0" />
         </font>
      </Label>
      <VBox alignment="CENTER" style="-fx-border-color: black;">
         <children>
            <ImageView fx:id="prizeImage" fitHeight="400.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../couponunknown.png" />
               </image>
            </ImageView>
         </children>
      </VBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
